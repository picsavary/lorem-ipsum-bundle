{
    "name": "amps/lorem-ipsum-bundle",
    "description": "Happy Lorem Ipsum for my web sites",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "anne-marie savary",
            "email": "picsavary@icloud.com"
        }
    ],
    "require": {
        "php": "^8.1.4",
        "symfony/config": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/flex": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Amps\\LoremIpsumBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amps\\LoremIpsumBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/browser-kit": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
